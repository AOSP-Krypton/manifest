<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="krypton"
          fetch="https://github.com/AOSP-Krypton/"
          revision="A11" />

  <remote name="aosp"
          fetch="https://android-review.googlesource.com/"
          revision="refs/tags/android-11.0.0_r35" />

  <default remote="aosp" />

  <remote name="caf"
          fetch="https://source.codeaurora.org/quic/la/" />

  <remote name="krypton-gitlab"
          fetch="https://gitlab.com/kosp/" />

  <remote name="lineage"
          fetch="https://github.com/LineageOS"
          revision="lineage-18.1" />

  <!-- Track aosp repos -->
  <include name="aosp.xml" />

  <!-- Untrack certain aosp repos -->
  <include name="remove.xml" />

  <!-- Track krypton repos -->
  <include name="krypton.xml" />

  <!-- Track opengapps -->
  <include name="gapps.xml" />

  <!-- Track caf hals -->
  <include name="caf.xml" />
</manifest>
