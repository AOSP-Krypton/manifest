<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="krypton"
          fetch="ssh://git@github.com/AOSP-Krypton/"
          revision="A11" />

  <remote name="aosp"
          fetch="https://android-review.googlesource.com/"
          revision="refs/tags/android-11.0.0_r45" />

  <default remote="aosp" />

  <remote name="caf_qssi"
          fetch="https://source.codeaurora.org/quic/la/"
          revision="refs/tags/LA.QSSI.11.0.r1-13000.02-qssi.0" />

  <remote name="caf_vendor"
          fetch="https://source.codeaurora.org/quic/la/"
          revision="refs/tags/LA.UM.9.1.r1-11100-SMxxx0.0" />

  <remote name="caf_ant"
          fetch="https://source.codeaurora.org/quic/la/"
          revision="refs/tags/LA.UM.9.2.1.r1-07900-sdm660.0" />

  <remote name="krypton-gitlab"
          fetch="ssh://git@gitlab.com/kosp/" />

  <remote name="clang-gitlab"
          fetch="ssh://git@gitlab.com/zlatanr/" />

  <remote name="lineage"
          fetch="https://github.com/LineageOS"
          revision="lineage-18.1" />

  <remote name="proton"
          fetch="https://github.com/ProtonAOSP/"
          revision="rvc" />

  <!-- Track aosp repos -->
  <include name="snippets/aosp.xml" />

  <!-- Untrack certain aosp repos -->
  <include name="snippets/remove.xml" />

  <!-- Track krypton repos -->
  <include name="snippets/krypton.xml" />

  <!-- Track opengapps -->
  <include name="snippets/gapps.xml" />

  <!-- Track caf hals -->
  <include name="snippets/caf.xml" />

  <!-- Track lineage repos -->
  <include name="snippets/lineage.xml" />
</manifest>
