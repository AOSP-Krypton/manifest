<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="krypton"
        fetch="https://github.com/AOSP-Krypton/"
        revision="refs/heads/A12" />

    <project path="build/make" name="build_make" remote="krypton" >
        <copyfile src="core/root.mk" dest="Makefile" />
        <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
        <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
        <linkfile src="core" dest="build/core" />
        <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
        <linkfile src="target" dest="build/target" />
        <linkfile src="tools" dest="build/tools" />
    </project>
    <project path="build/soong" name="build_soong" remote="krypton" >
        <linkfile src="root.bp" dest="Android.bp" />
        <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
    </project>

    <!-- Lineage sepolicy -->
  <project path="device/lineage/sepolicy" name="android_device_lineage_sepolicy" remote="krypton" revision="A11" />

  <!-- Hardware -->
  <project path="hardware/lineage/interfaces" name="android_hardware_lineage_interfaces" remote="krypton" revision="A11" />

  <!-- Prebuilts -->
  <project path="prebuilts/krypton-tools" name="prebuilts_krypton-tools" remote="krypton" revision="A11" />

  <!-- Packages -->
  <project path="packages/apps/KOSP-Updater" name="packages_apps_KOSP-Updater" remote="krypton" />

  <!-- Vendor -->
  <project path="vendor/krypton" name="vendor_krypton" remote="krypton" />

  <!-- QCOM Vendor -->
  <project path="vendor/qcom/opensource/commonsys/bluetooth_ext" name="android_vendor_qcom_opensource_bluetooth_ext" remote="krypton" revision="A11" />
  <project path="vendor/qcom/opensource/commonsys/packages/apps/Bluetooth" name="android_vendor_qcom_opensource_packages_apps_Bluetooth" remote="krypton" revision="A11" />
</manifest>